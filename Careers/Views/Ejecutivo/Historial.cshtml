

@model Careers.Domain.Ejecutivos


@{
    ViewBag.Title = "Historial - Citas de los Ejecutivos";
}
<br />
<ul class="nav nav-pills">
    <li class="nav-item">
        <h4>Historial de las Citas completadas y Canceladas de los ejecutivos</h4>
    </li>
</ul>
<br />

<table class="table table-striped table-bordered dt-responsive nowrap" style="" id="tbl-perfil-ejecutivos">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Ejecutivo</th>
            <th scope="col">Empresa</th>
            <th scope="col">Colaborador</th>
            <th scope="col">Empresa</th>
            <th scope="col">Area</th>
            <th scope="col">Fecha y Hora</th>
            <th scope="col">Canal</th>
            <th scope="col">Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.Datos)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Ejecutivo</td>
                <td>@item.Empresa</td>
                <td>@item.Colaborador</td>
                <td>@item.EmpresaColaborador</td>
                <td>@item.Area</td>
                <td>@item.Fecha</td>
                <td>@item.Canal</td>
                <td>@item.Estatus</td>

            </tr>
        }
    </tbody>
</table>
<br />
<br />

@section Scripts {
    <script src="~/Content/datepicker/jquery.js"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.19/js/dataTables.bootstrap4.min.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.2.3/js/dataTables.responsive.min.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.2.3/js/responsive.bootstrap4.min.js"></script>

    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/1.10.19/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="https://cdn.datatables.net/responsive/2.2.3/css/responsive.bootstrap4.min.css" rel="stylesheet" />

    <script type="text/javascript">
        $(document).ready(function () {
            $('#tbl-perfil-ejecutivos').dataTable();
        });
    </script>
}